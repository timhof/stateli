
<div id="account_pending" class="template_div">
	<div class="date_selection_div">
		<%= render :partial => 'date_span_selector_pending', :locals => {:form_url => transactions_pending_path, :start_text_field_id => "start_date", :end_text_field_id => "end_date"}; %>
	</div>
	<div class="flex_spacer_div">&nbsp;</div>
	<div class="table_div">
		<div id="transactions_pending_credit" class="type_div" style="display:<%= @transactions.size > 0 ? "block" : "none" %>;">

		  	<%= render :partial => 'pending_header' %>
		  	<div>
		  		<%= render :partial => 'pending_details', :collection => @transactions %>
		  	</div>
		</div>
	</div>
</div>