
<div id="pending" class="template_div">
	<div class="page_title_div">
		<span id="contract_name_<%= @contract.id %>" class="contract_name"><%= @contract.name%> <%= render :partial => 'options', :locals => {:contract => @contract} %></span>
		<%= render :partial => 'stateli/date_span_selector_pending', :locals => {:form_url => contract_pending_path(@contract), :start_text_field_id => "start_date", :end_text_field_id => "end_date"}; %>
	</div>
	<%= render :partial => 'transactions/transactions_div', :locals => {:transactions => @transactions, :div_id => :contract_transactions_pending} %>
</div>